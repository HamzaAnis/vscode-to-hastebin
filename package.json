{
    "name": "vscode-to-hastebin",
    "displayName": "VSCode to Hastebin",
    "description": "This extension will let you share files to the hastebin (https://hastebin.com)",
    "version": "0.0.1",
    "publisher": "Hamzaanis",
    "repository": {
        "type": "git",
        "url": "https://github.com/HamzaAnis/vscode-to-hastebin"
    },
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vscodetohaste.share"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "vscodetohaste.share",
            "title": "HasteBin: Share on hastebin"
        }],
        "keybindings": [{
            "command": "vscodetohaste.share",
            "key": "ctrl+alt+h"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "axios": "^0.18.0"
    }
}